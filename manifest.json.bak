{
  "name": "GraphQL Environments",
  "description": "Change environments on GraphQL playground, add global variable to authorization bearer and login with AWS cognito",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
      "16": "/images/icon_32.png",
      "32": "/images/icon_32.png",
      "48": "/images/icon_32.png",
      "128": "/images/icon_128.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
          "16": "/images/icon_32.png",
          "32": "/images/icon_32.png",
          "48": "/images/icon_32.png",
          "128": "/images/icon_128.png"
      }
  },
  "options_page": "options.html",
  "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
  ],
  "host_permissions": [
      "https://www.google.com/*"
  ]
}